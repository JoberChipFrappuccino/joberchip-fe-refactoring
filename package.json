{
  "scripts": {
    "prepare": "husky install",
    "build:dev:server": "cross-env NODE_ENV=development webpack --config webpack.server.config.js --mode development --progress --profile",
    "build:dev:client": "cross-env NODE_ENV=development webpack --config webpack.client.config.js --mode development --progress --profile",
    "build:dev": "node ./scripts/rm.js && npm run build:dev:client && npm run build:dev:server",
    "build:prod:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js --mode production --progress --profile",
    "build:prod:client": "cross-env NODE_ENV=production webpack --config webpack.client.config.js --mode production --progress --profile",
    "build:prod": "node ./scripts/rm.js && npm run build:prod:client && npm run build:prod:server",
    "dev": "cross-env NODE_ENV=development node ./dist/index.js",
    "dev:pm2": "pm2 start ./ecosystem.config.js --only dev-server --env development",
    "build:dev:server:watch": "cross-env NODE_ENV=development webpack --config webpack.server.config.js --mode development --progress --profile --watch",
    "dev:server:watch": "cross-env NODE_ENV=development nodemon --watch \"./dist/index.js\" --exec \"npm run dev\"",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "start:pm2": "pm2 start ./ecosystem.config.js --only prod-server --env production",
    "del": "pm2 del all"
  },
  "dependencies": {
    "@loadable/component": "^5.15.3",
    "@loadable/server": "^5.16.1",
    "@types/react-helmet": "^6.1.6",
    "antd": "^5.8.6",
    "axios": "^1.5.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "html-entities-decoder": "^1.0.2",
    "loadable": "^1.0.0",
    "nodemon": "^3.0.1",
    "pm2": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.3.4",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.46.0",
    "react-router-dom": "^6.15.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@loadable/babel-plugin": "^5.16.1",
    "@loadable/webpack-plugin": "^5.15.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/html-entities-decoder": "^1.0.0",
    "@types/loadable__component": "^5.13.4",
    "@types/loadable__server": "^5.12.6",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-grid-layout": "^1.3.2",
    "@types/webpack-env": "^1.18.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-react-app": "^10.0.1",
    "css-loader": "^6.8.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "typescript-plugin-css-modules": "^5.0.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "^2.25.4",
    "webpack-node-externals": "^3.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
